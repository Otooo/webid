{
  "name": "webbid-program",
  "version": "1.0.0",
  "private": true,
  "description": "Web bid program",
  "author": "Oto Lopes <otoengc@gmail.com>",
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/recommended"
    ],
    "rules": {
      "indent": [
        "error",
        2
      ],
      "semi": [
        "warn",
        "always"
      ],
      "quotes": [
        "warn",
        "single"
      ]
    }
  },
  "eslintIgnore": [
    "vue.config.js",
    "intelij.webpack.js",
    "dist/*",
    "public/*",
    "node_modules/*"
  ],
  "scripts": {
    "serve": "set NODE_ENV=dev; vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "start": "node server.js",
    "unit": "node test/e2e/runner.js",
    "lint": "vue-cli-service lint --no-fix",
    "lint-fix": "vue-cli-service lint --fix"
  },
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.0.0-rc.25",
    "chart.js": "^2.8.0",
    "express": "^4.17.1",
    "moment": "^2.24.0",
    "register-service-worker": "^1.5.2",
    "serve-static": "^1.14.1",
    "sweetalert": "^2.1.2",
    "v-money": "^0.8.1",
    "v-tooltip": "^2.0.2",
    "vue": "^2.6.10",
    "vue-chartjs": "^3.4.2",
    "vue-network": "^0.1.1",
    "vue-router": "^3.0.2",
    "vue-the-mask": "^0.11.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-plugin-pwa": "^3.4.0",
    "@vue/cli-service": "^4.3.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.3.1",
    "vue-template-compiler": "^2.6.6"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 11"
  ]
}
